    <string name="notification_direct_message_multiple_messages">Si <xliff:g id="user">%1$s</xliff:g> ay nagpadala sayo ng <xliff:g id="messages_count">%2$d</xliff:g> na mga direktang mensahe.</string>
    <string name="notification_direct_message_multiple_users"><xliff:g id="user">%1$s</xliff:g> at <xliff:g id="users_count">%2$d</xliff:g> ang iba ay pinadalhan ka ng <xliff:g id="messages_count">%3$d</xliff:g> na direktang mga mensahe.</string>
